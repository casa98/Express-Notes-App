<div class="row">
    {{#each notes}}
        <div class="col-md-4">
            <div class="card">
                <div class="card-body">
                    <h4 class="card-title">
                        {{title}}
                    </h4>
                    <p>
                        {{description}}
                    </p>
                </div>
            </div>
        </div>
    {{/each}}
</div>